<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.nfc.share" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NFC Share</name>
    <description>
        A sample Apache Cordova application to test NFC peer to peer sharing.
    </description>
    <author href="http://github.com/don">
        Don Coleman
    </author>
    <content src="index.html" />
    <access origin="*" />
    <platform name="android">
        <preference name="android-targetSdkVersion" value="22" />
    </platform>
    <plugin name="phonegap-nfc" spec="~0.6.6" />
</widget>
